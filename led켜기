const int led=9; //신호값
void setup() {
  // put your setup code here, to run once:
pinMode(led,OUTPUT); //led 9번 아웃풋

 
}

void loop() {
  // put your main code here, to run repeatedly:
  for(int i=100;i<1000;i+=100) // delay를 늘리기 위해 for문
  {
    digitalWrite(led,HIGH);
    delay(i); 
    digitalWrite(led,LOW);
    delay(i);
  }
}
